{
  "version": 2,
  "name": "resume-screener-frontend",
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/build",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
