# OpenAI API Key for AI-powered resume analysis
# Get your API key from: https://platform.openai.com/api-keys
# IMPORTANT: Replace the value below with your actual OpenAI API key
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
PORT=5000
CLIENT_URL=http://localhost:3000

# Node Environment
NODE_ENV=development

# Redis Configuration (Optional - for enhanced caching and job queues)
# If not provided, application will use in-memory caching
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Performance Settings
MAX_FILE_SIZE=5242880
MAX_BATCH_SIZE=52428800
CACHE_TTL=3600
JOB_QUEUE_CONCURRENCY=5

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=50
ANALYSIS_RATE_LIMIT=5
BULK_ANALYSIS_RATE_LIMIT=2
